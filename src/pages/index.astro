---
const search =
  Astro.url.searchParams.get('search') || ''

import Main from '../components/main.astro'

import { data } from '../data'

let { en, es, fr, de } = data(search)
const myFilter = table =>
  table.filter(el => el[2])
en = myFilter(en)
es = myFilter(es)
fr = myFilter(fr)
---

<Main
  en={en}
  es={es}
  fr={fr}
  de={de}
  search={search}
/>
